#Requires AutoHotkey v2
#SingleInstance Force
Installer := Gui(, 'Sheep Vs Wolf 3')
Installer.OnEvent('Close', (*) => ExitApp())
Installer.BackColor := 'White'
Installer.MarginX := 10
Installer.MarginY := 10
Installer.SetFont('s30')
Installer.AddText('w300', 'Sheep Vs Wolf 3')
Banner := ''
Data := [ 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/7za.exe'                   ; Unpacker
        , 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/002.7z.001'                ; Patchs
        , 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/003.7z.001'                ; AoK Part 1
        , 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/003.7z.002'                ; AoK Part 2
        , 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/003.7z.003'                ; AoK Part 3
        , 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/003.7z.004'                ; AoK Part 4
        , 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/004.7z.001'                ; AoC Part 1
        , 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/004.7z.002'                ; AoC Part 2
        , 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/004.7z.003'                ; AoC Part 3
        , 'https://github.com/Chandoul/aoeii_vanilla_patchs_mods/raw/main/015.7z.001' ] ; Sheep Vs Wolf 3
Try {
    Installer.AddPicture('wp', 'HBITMAP:*' hBitmapFromB64(Banner))
}
Installer.SetFont('s10')
Installer.AddText('cGreen wp Center', 'AoE II data mod for offline use')
PB := Installer.AddProgress('-Smooth wp h18')
Install := Installer.AddButton('xm+50 w200', 'Install')
Installer.Show()
Install.OnEvent('Click', InstallMod)
InstallMod(Ctrl, Info) {
    Ctrl.Enabled := False
    If !InstallLocation := FileSelect('D', A_Desktop, 'Sheep Vs Wolf 3') {
        Return
    }
    Try {
		InstallLocation .= '\Sheep Vs Wolf 3\InstallationFiles'
        If !DirExist(InstallLocation)
            DirCreate(InstallLocation)
		; Download
		EstimatedSize := 768 ; in MB
		SetTimer(WatchProgress, 1000)
		WatchProgress() {
			Size := 0
			Loop Files, InstallLocation '\*.*', 'R' {
				Size += FileGetSize(A_LoopFileFullPath, 'M')
			}
			PB.Value := Round((Size / EstimatedSize) * 100, 2)
			Ctrl.Text := 'Installing...(' PB.Value ' %)'
		}
		For Each, Link in Data {
			File := StrReplace(Link, 'https://github.com/SmileAoE/aoeii_aio/raw/main/DB/')
			File := StrReplace(File, 'https://github.com/Chandoul/aoeii_vanilla_patchs_mods/raw/main/')
			If !FileExist(InstallLocation '\' File) {
				Download(Link, InstallLocation '\' File)
			}
		}
		; Install
		Unpacker := InstallLocation '\7za.exe'
		ExtractPackage(InstallLocation '\002.7z.001', InstallLocation '\002')
		ExtractPackage(InstallLocation '\003.7z.001', InstallLocation '\003')
		ExtractPackage(InstallLocation '\004.7z.001', InstallLocation '\004')
		ExtractPackage(InstallLocation '\015.7z.001', InstallLocation '\015')
		ExtractPackage(PackagePath, PackageFolder, Overwrite := 0) {
			Overwrite := Overwrite ? Overwrite : !DirExist(PackageFolder)
			If Overwrite && FileExist(PackagePath) {
				RunWait('"' Unpacker '" x "' PackagePath '" -o"' PackageFolder '" -aoa',, 'Hide')
			}
		}
		SetTimer(WatchProgress, 0)
		PB.Value := 100
		Ctrl.Text := 'Applying...'
		SplitPath(InstallLocation,, &InstallLocationA)
		DirCopy(InstallLocation '\003', InstallLocationA, 1)
		DirCopy(InstallLocation '\004', InstallLocationA, 1)
		CleansUp()
		; Cleans up
		CleansUp() {
		    Loop Files, InstallLocation '\002\*', 'D' {
		        Edition := A_LoopFileName
				Loop Files, InstallLocation '\002\' Edition '\*', 'D' {
					Version := A_LoopFileName
		        	Loop Files, InstallLocation '\002\' Edition '\' Version '\*.*', 'R' {
		        	    PathFile := StrReplace(A_LoopFileDir '\' A_LoopFileName, InstallLocation '\002\' Edition '\' Version '\')
		        	    If FileExist(InstallLocationA '\' PathFile) {
		        	        FileDelete(InstallLocationA '\' PathFile)
		        	    }
		        	}
				}
		    }
		}
		DirCopy(InstallLocation '\002\aoc\1.0c', InstallLocationA, 1)
		DirCopy(InstallLocation '\002\aoc\1.5', InstallLocationA, 1)
		DirCopy(InstallLocation '\015', InstallLocationA, 1)
    } Catch {
		PB.Value := 0
    	Ctrl.Enabled := True
    	Ctrl.Text := 'Install'
		MsgBox('An error occured during the installation`nPlease try with a new clean installation setup', 'Installation error!', 0x10)
		Return
	}
	PB.Value := 0
    Ctrl.Enabled := True
    Ctrl.Text := 'Install'
	Msgbox('Installation complete!`nYou find your game at:`n`n' InstallLocation, 'Complete!', 0x40)
}
Gdip_Startup() {
	if (!DllCall("LoadLibrary", "str", "gdiplus", "UPtr")) {
		throw Error("Could not load GDI+ library")
	}

	si := Buffer(A_PtrSize = 4 ? 20:32, 0) ; sizeof(GdiplusStartupInputEx) = 20, 32
	NumPut("uint", 0x2, si)
	NumPut("uint", 0x4, si, A_PtrSize = 4 ? 16:24)
	DllCall("gdiplus\GdiplusStartup", "UPtr*", &pToken:=0, "Ptr", si, "UPtr", 0)
	if (!pToken) {
		throw Error("Gdiplus failed to start. Please ensure you have gdiplus on your system")
	}
	return pToken
}
Gdip_Shutdown(pToken) {
	DllCall("gdiplus\GdiplusShutdown", "UPtr", pToken)
	hModule := DllCall("GetModuleHandle", "str", "gdiplus", "UPtr")
	if (!hModule) {
		throw Error("GDI+ library was unloaded before shutdown")
	}
	if (!DllCall("FreeLibrary", "UPtr", hModule)) {
		throw Error("Could not free GDI+ library")
	}
	return 0
}
Gdip_BitmapFromBase64(Base64) {
	; calculate the length of the buffer needed
	if !(DllCall("crypt32\CryptStringToBinary", "UPtr", StrPtr(Base64), "UInt", 0, "UInt", 0x01, "UPtr", 0, "UInt*", &DecLen:=0, "UPtr", 0, "UPtr", 0)) {
		return -1
	}
	Dec := Buffer(DecLen, 0)
	; decode the Base64 encoded string
	if !(DllCall("crypt32\CryptStringToBinary", "UPtr", StrPtr(Base64), "UInt", 0, "UInt", 0x01, "UPtr", Dec.Ptr, "UInt*", &DecLen, "UPtr", 0, "UPtr", 0)) {
		return -2
	}
	; create a memory stream
	if !(pStream := DllCall("shlwapi\SHCreateMemStream", "UPtr", Dec.Ptr, "UInt", DecLen, "UPtr")) {
		return -3
	}
	DllCall("gdiplus\GdipCreateBitmapFromStreamICM", "UPtr", pStream, "Ptr*", &pBitmap:=0)
	ObjRelease(pStream)
	return pBitmap
}
hBitmapFromB64(b64Image) {
    pToken := Gdip_Startup()
	If (B := Gdip_BitmapFromBase64(b64Image)) > -1 {
		Return Gdip_CreateHBITMAPFromBitmap(B)
	}
    Gdip_Shutdown(pToken)
}
Gdip_CreateHBITMAPFromBitmap(pBitmap, Background:=0xffffffff)
{
	DllCall("gdiplus\GdipCreateHBITMAPFromBitmap", "UPtr", pBitmap, "UPtr*", &hbm:=0, "Int", Background)
	return hbm
}
